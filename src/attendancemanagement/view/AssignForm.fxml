<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" stylesheets="@../res/style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="attendancemanagement.controller.AssignFormController">
   <children>
      <HBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <VBox>
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="35.0">
                     <children>
                        <Label fx:id="assignLabel" text="ASSIGN" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="40.0" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <HBox fx:id="assignBox" alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="200.0" stylesheets="@../res/style.css" />
                  <HBox alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="40.0">
                     <children>
                        <Button fx:id="submitButton" mnemonicParsing="false" onAction="#assign" styleClass="label-button" text="ASSIGN" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
